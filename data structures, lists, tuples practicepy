list_one = [1, 2, 3]

list_one.index(2)
Out[31]: 1

list_one.index(3)
Out[32]: 2

list_one.index(1)
Out[33]: 0

a=list_one

a.append(2)

a.count(2)
Out[36]: 2

a.count(1)
Out[37]: 1

a
Out[38]: [1, 2, 3, 2]

del a[3]

a
Out[40]: [1, 2, 3]

a.remove(2)

a
Out[42]: [1, 3]

a=a*3

a
Out[44]: [1, 3, 1, 3, 1, 3]

a.count(3)
Out[45]: 3

a.count(1)
Out[46]: 3

a.count(2)
Out[47]: 0

nlist = ['1000','93','94','1']
print(max(nlist))
print(min(nlist))

94
1

